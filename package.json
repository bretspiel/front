  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "fmt": "prettier --parser typescript 'src/**/*.ts{,x}' --write",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "run-p test:*",
    "build": "cross-env NODE_ENV=production run-s test clean build:transpile",
